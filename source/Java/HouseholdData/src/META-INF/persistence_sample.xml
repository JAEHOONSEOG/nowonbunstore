	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="HouseholdData">
		<class>model.Ctgry</class>
		<class>model.Hshld</class>
		<class>model.HshldLog</class>
		<class>model.HshldRelation</class>
		<class>model.SysDt</class>
		<class>model.TknDmn</class>
		<class>model.Tp</class>
